my_useless_function := proc (a : int, b : int, c : int, d : int, e : int, f : float, s : string) void {

    print(a << 1); # double
    print((b << 1) + b); # triple
    print(c >> 1); # half
    print(float(d + e) * f);
    print(s);
};

main := proc () void {
    my_useless_function(1, 2, 3, 4, 5, .6, "7");
    my_useless_function(7, 6, 5, 1, 3, 2., "4");
};
